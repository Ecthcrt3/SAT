@model SAT.DATA.EF.Models.StudentStatus

@{
    ViewData["Title"] = "Edit";
}

<section class="contact_section layout_padding">

    <div class="heading_container my-4">
        <h2>
            @Model.Ssid
        </h2>

    </div>
    <div class="">
        <div class="px-5">
            <div class="row justify-content-center">
                <div class="col-md-4">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="Ssid" />
                        <div class="form-group">
                            <label asp-for="Ssname" class="control-label"></label>
                            <input asp-for="Ssname" class="form-control" />
                            <span asp-validation-for="Ssname" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Ssdescription" class="control-label"></label>
                            <input asp-for="Ssdescription" class="form-control" />
                            <span asp-validation-for="Ssdescription" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Save" class="btn btn-dark" />
                            <a class="btn btn-dark" asp-action="Index">Cancel</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>





@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
