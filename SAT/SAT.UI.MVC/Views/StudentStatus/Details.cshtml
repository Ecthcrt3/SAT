@model SAT.DATA.EF.Models.StudentStatus

@{
    ViewData["Title"] = "Details";
}

<section class="contact_section layout_padding">

    <div class="heading_container my-4">
        <h2>
            @Model.Ssid
        </h2>

    </div>
    <div class="">
        <div class="px-5">
            <div class="row justify-content-center">
                <div class="col-md-4">
                    <div>
                        <h4>Student Status</h4>
                        <hr />
                        <dl class="row">
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Ssname)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Ssname)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Ssdescription)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Ssdescription)
                            </dd>
                        </dl>
                    </div>
                    <div>
                        <a class="btn btn-dark" asp-action="Edit" asp-route-id="@Model.Ssid">Edit</a>
                        <a class="btn btn-dark" asp-action="Index">Cancel</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>


